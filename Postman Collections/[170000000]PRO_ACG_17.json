            "body": {
              "mode": "raw",
              "raw": "{  \r\n   \"sqlRequest\":[  \r\n      {  \r\n         \"requestId\":\"CheckAlertePCEMJ\",\r\n         \"requestContent\":\"select coalesce( (select count(*) as COUNT from alerte, pce,vpce,test_ref where alerte.uidgrandeurs_objet=pce.uidgrandeurs_objet and alerte.uidtest_ref=test_ref.uidtest_ref and pce.uidpce=vpce.uidpce AND vpce.DATE_FIN_AFFICHEE is null and code_test='INIT' and alerte.lstime>=(sysdate-1) and alerte.lstime<=sysdate and type_pce='M' and sous_type='J'),0) as count from dual\"\r\n      },\r\n     {  \r\n         \"requestId\":\"CheckAlertePCEPoint\",\r\n         \"requestContent\":\"select coalesce ((select count(*) COUNT from alerte, pce,vpce,test_ref where alerte.uidgrandeurs_objet=pce.uidgrandeurs_objet and alerte.uidtest_ref=test_ref.uidtest_ref and pce.uidpce=vpce.uidpce AND vpce.DATE_FIN_AFFICHEE is null and code_test='INIT' and source_acquisition in ('500','703') and alerte.lstime>=(sysdate-1) and alerte.lstime<=sysdate ),0) as count from dual\"\r\n      },     \r\n\t  {  \r\n         \"requestId\":\"CheckAlertePCESourceAcquisition\",\r\n         \"requestContent\":\"select coalesce ((select count(*) COUNT from alerte, pce,vpce,test_ref where alerte.uidgrandeurs_objet=pce.uidgrandeurs_objet and alerte.uidtest_ref=test_ref.uidtest_ref and pce.uidpce=vpce.uidpce AND vpce.DATE_FIN_AFFICHEE is null and code_test='INIT' and type_point in ('T','8') and alerte.lstime>=(sysdate-1) and alerte.lstime<=sysdate),0) as count from dual\"\r\n      },   \r\n  \t  {  \r\n         \"requestId\":\"CheckAlertePCE\",\r\n         \"requestContent\":\"select type_pce,sous_type, count(*) as count from alerte, pce,vpce,test_ref where alerte.uidgrandeurs_objet=pce.uidgrandeurs_objet and alerte.uidtest_ref=test_ref.uidtest_ref and pce.uidpce=vpce.uidpce AND vpce.DATE_FIN_AFFICHEE is null and code_test='INIT' and ( source_acquisition  not in ('T','8') or type_point in ('500','703') or (type_pce='M' and sous_type='J')) and alerte.lstime>=(sysdate-1) and alerte.lstime<=sysdate group by type_pce,sous_type\"\r\n      }\r\n   ]\r\n}"
            },
            "description": ""
